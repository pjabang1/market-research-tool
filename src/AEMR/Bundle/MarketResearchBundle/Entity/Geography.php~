<?php

namespace AEMR\Bundle\MarketResearchBundle\Entity;

use Doctrine\ORM\Mapping as ORM;

/**
 * Geography
 */
class Geography
{
    /**
     * @var string
     */
    private $code;

    /**
     * @var string
     */
    private $name;

    /**
     * @var string
     */
    private $type;

    /**
     * @var integer
     */
    private $id;


    /**
     * Set code
     *
     * @param string $code
     * @return Geography
     */
    public function setCode($code)
    {
        $this->code = $code;

        return $this;
    }

    /**
     * Get code
     *
     * @return string 
     */
    public function getCode()
    {
        return $this->code;
    }

    /**
     * Set name
     *
     * @param string $name
     * @return Geography
     */
    public function setName($name)
    {
        $this->name = $name;

        return $this;
    }

    /**
     * Get name
     *
     * @return string 
     */
    public function getName()
    {
        return $this->name;
    }

    /**
     * Set type
     *
     * @param string $type
     * @return Geography
     */
    public function setType($type)
    {
        $this->type = $type;

        return $this;
    }

    /**
     * Get type
     *
     * @return string 
     */
    public function getType()
    {
        return $this->type;
    }

    /**
     * Get id
     *
     * @return integer 
     */
    public function getId()
    {
        return $this->id;
    }
    /**
     * @var string
     */
    private $numerical_code;


    /**
     * Set numerical_code
     *
     * @param string $numericalCode
     * @return Geography
     */
    public function setNumericalCode($numericalCode)
    {
        $this->numerical_code = $numericalCode;

        return $this;
    }

    /**
     * Get numerical_code
     *
     * @return string 
     */
    public function getNumericalCode()
    {
        return $this->numerical_code;
    }
    /**
     * @var string
     */
    private $code_3;


    /**
     * Set code_3
     *
     * @param string $code3
     * @return Geography
     */
    public function setCode3($code3)
    {
        $this->code_3 = $code3;

        return $this;
    }

    /**
     * Get code_3
     *
     * @return string 
     */
    public function getCode3()
    {
        return $this->code_3;
    }
}
