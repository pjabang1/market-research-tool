<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping>
    <entity name="AEMR\Bundle\MarketResearchBundle\Entity\GeoGroup" table="base_geogroups" repository-class="AEMR\Bundle\MarketResearchBundle\Repository\GeoGroupRepository">
	<change-tracking-policy>DEFERRED_IMPLICIT</change-tracking-policy>
	<id name="id" type="bigint" column="id">
	    <generator strategy="IDENTITY"/>
	</id>
	<field name="name" type="string" column="name" length="255" />
	<field name="type" type="string" column="type" length="50" />
	<field name="description" type="string" column="description" length="255" />
	<indexes>
	    <index name="type" columns="type" />
	</indexes>
	<unique-constraints>
	</unique-constraints>
	<lifecycle-callbacks/>
    </entity>
</doctrine-mapping>