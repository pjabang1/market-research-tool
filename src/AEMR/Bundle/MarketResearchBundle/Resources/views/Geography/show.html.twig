{% extends '::base.html.twig' %}

{% block body -%}
    <h1>Geography</h1>

    <table class="record_properties">
        <tbody>
            <tr>
                <th>Code</th>
                <td>{{ entity.code }}</td>
            </tr>
            <tr>
                <th>Code_3</th>
                <td>{{ entity.code3 }}</td>
            </tr>
            <tr>
                <th>Numerical_code</th>
                <td>{{ entity.numericalcode }}</td>
            </tr>
            <tr>
                <th>Name</th>
                <td>{{ entity.name }}</td>
            </tr>
            <tr>
                <th>Type</th>
                <td>{{ entity.type }}</td>
            </tr>
            <tr>
                <th>Id</th>
                <td>{{ entity.id }}</td>
            </tr>
        </tbody>
    </table>

        <ul class="record_actions">
    <li>
        <a href="{{ path('geography') }}">
            Back to the list
        </a>
    </li>
    <li>
        <a href="{{ path('geography_edit', { 'id': entity.id }) }}">
            Edit
        </a>
    </li>
    <li>{{ form(delete_form) }}</li>
</ul>
{% endblock %}
